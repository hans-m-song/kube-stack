{
  "name": "kube-stack",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "src/index.ts",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "src:compile": "esbuild src/index.ts --bundle --minify --sourcemap --platform=node --outfile=dist/index.js",
    "src:watch": "yarn src:compile --watch",
    "types:compile": "tsc --noEmit",
    "types:watch": "yarn types:compile --watch",
    "watch": "concurrently -r \"yarn:src:watch\" \"yarn:types:watch\"",
    "synth": "node dist/index.js",
    "build": "yarn types:compile && yarn src:compile && yarn synth",
    "lint": "eslint \"src/**/*.ts\"",
    "diff": "kubectl diff -f manifests || true > pending.diff",
    "diff:ss": "kubectl diff -f manifests --server-side || true > pending.diff",
    "clean": "rm -rf manifests"
  },
  "dependencies": {
    "cdk8s": "^2.3.17",
    "constructs": "^10.1.105",
    "dotenv": "^16.0.1",
    "esbuild": "^0.14.46",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/node": "^14.18.20",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "cdk8s-cli": "^2.0.15",
    "concurrently": "^7.2.2",
    "eslint": "^8.18.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.8.1",
    "tsx": "^3.9.0",
    "typescript": "^4.7.3"
  }
}
